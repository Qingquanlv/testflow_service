<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.qingquanlv.testflow_service_api.mapper.PaserCaseMapper">
    <resultMap id="PaserCase" type="com.github.qingquanlv.testflow_service_api.entity.testflow_service_db.PaserCase">
        <result column="case_id" jdbcType="BIGINT" property="case_id" />
        <result column="case_name" jdbcType="VARCHAR" property="case_name" />
        <result column="parameters" jdbcType="VARCHAR" property="parameters" />
        <result column="cvt_method_source" jdbcType="VARCHAR" property="cvt_method_source" />
        <result column="return_type" jdbcType="VARCHAR" property="return_type" />
        <result column="datachanged_lasttime" jdbcType="TIMESTAMP" property="datachanged_lasttime" />
    </resultMap>

    <select id="Sel" resultType="com.github.qingquanlv.testflow_service_api.entity.testflow_service_db.PaserCase">
        SELECT * FROM paser_case where case_name = #{name}
    </select>

    <delete id="Del" >
        DELETE FROM testflow_service.database_case where case_id = #{id}
    </delete>

    <insert id="Insert" parameterType="java.util.List" useGeneratedKeys="false">
        insert into paser_case
        ( case_name,parameters,cvt_method_source,return_type)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (
            #{item.case_name},
            #{item.parameters},
            #{item.cvt_method_source},
            #{item.return_type}
            )
        </foreach>
    </insert>

    <update id="Update" parameterType="com.github.qingquanlv.testflow_service_api.entity.testflow_service_db.PaserCase">
        SELECT * FROM testflow_service.database_case where case_id = #{id}
    </update>

</mapper>